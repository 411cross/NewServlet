<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd"
         version="3.1">

    <servlet>
        <servlet-name>userLogin</servlet-name>
        <servlet-class>com.company.Servlet.UserLoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>adminLogin</servlet-name>
        <servlet-class>com.company.Servlet.AdminLoginServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>nurseFilter</servlet-name>
        <servlet-class>com.company.Servlet.FilterNurseServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>register</servlet-name>
        <servlet-class>com.company.Servlet.RegisterServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>modifyUserInformation</servlet-name>
        <servlet-class>com.company.Servlet.ModifyUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addRelation</servlet-name>
        <servlet-class>com.company.Servlet.AddFamilyRelationServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>createOrder</servlet-name>
        <servlet-class>com.company.Servlet.CreateOrderServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>getOrder</servlet-name>
        <servlet-class>com.company.Servlet.GetOrderServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>getPatient</servlet-name>
        <servlet-class>com.company.Servlet.GetPatientServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addNurse</servlet-name>
        <servlet-class>com.company.Servlet.AddNurseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>modifyNurse</servlet-name>
        <servlet-class>com.company.Servlet.ModifyUserServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>getNurseList</servlet-name>
        <servlet-class>com.company.Servlet.GetNurseServlet</servlet-class>
    </servlet>





    <servlet>
        <servlet-name>searchNurse</servlet-name>
        <servlet-class>com.company.Servlet.SearchNurseServlet</servlet-class>
    </servlet>

    <servlet>
        <servlet-name>deleteNurse</servlet-name>
        <servlet-class>com.company.Servlet.DeleteNurseServlet</servlet-class>
    </servlet>


    <servlet-mapping>
        <servlet-name>userLogin</servlet-name>
        <url-pattern>/userlogin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>nurseFilter</servlet-name>
        <url-pattern>/nursefilter</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>adminLogin</servlet-name>
        <url-pattern>/adminlogin</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>register</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>modifyUserInformation</servlet-name>
        <url-pattern>/modifyuserinformation</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addRelation</servlet-name>
        <url-pattern>/addrelation</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>createOrder</servlet-name>
        <url-pattern>/createorder</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>getOrder</servlet-name>
        <url-pattern>/getorder</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>getPatient</servlet-name>
        <url-pattern>/getpatient</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>searchNurse</servlet-name>
        <url-pattern>/searchnurse</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>deleteNurse</servlet-name>
        <url-pattern>/deletenurse</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>addNurse</servlet-name>
        <url-pattern>/addnurse</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>modifyNurse</servlet-name>
        <url-pattern>/modifynurse</url-pattern>
    </servlet-mapping>

    <servlet-mapping>
        <servlet-name>getNurseList</servlet-name>
        <url-pattern>/getnurselist</url-pattern>
    </servlet-mapping>


</web-app>